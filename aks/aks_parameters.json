{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "resourceName": {
            "value": "aks_lab"
        },
        "location": {
            "value": "southeastasia"
        },
        "dnsPrefix": {
            "value": "akslab-dns"
        },
        "kubernetesVersion": {
            "value": "1.20.9"
        },
        "networkPlugin": {
            "value": "kubenet"
        },
        "enableRBAC": {
            "value": true
        },
        "enablePrivateCluster": {
            "value": false
        },
        "enableHttpApplicationRouting": {
            "value": true
        },
        "enableAzurePolicy": {
            "value": false
        },
        "networkPolicy": {
            "value": "calico"
        }
    }
}